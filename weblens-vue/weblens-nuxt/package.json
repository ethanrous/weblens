{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate && find ./.output/public -type f \\( -name \\*.js -o -name \\*.css \\) -exec gzip {} \\; -exec mv {}.gz {} \\;",
    "preview": "nuxt preview",
    "lint": "prettier --config ./.prettierrc.json --check '**/**.vue' '**/**.ts'; tsc --noEmit; eslint --ext .ts,.vue .",
    "lint:fix": "prettier --config ./.prettierrc.json --write '**/**.vue' '**/**.ts'",
    "postinstall": "nuxt prepare",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report ../../_build/playwright/report",
    "test:e2e:coverage": "open ../../_build/playwright/report/coverage/index.html",
    "clean": "rm -rf .nuxt .output dist"
  },
  "dependencies": {
    "@ethanrous/weblens-api": "link:../../api/ts",
    "@nuxt/eslint": "1.4.1",
    "@nuxt/image": "1.10.0",
    "@nuxt/kit": "^4.2.2",
    "@pinia/nuxt": "0.11.1",
    "@tabler/icons-vue": "^3.36.0",
    "@tailwindcss/vite": "^4.1.18",
    "@vueuse/components": "^13.9.0",
    "@vueuse/core": "^13.9.0",
    "axios": "^1.13.2",
    "eslint": "^9.39.2",
    "geojson": "^0.5.0",
    "hls.js": "^1.6.15",
    "maplibre-gl": "^5.15.0",
    "mitt": "^3.0.1",
    "nuxt": "^3.20.2",
    "pinia": "^3.0.4",
    "socket.io-client": "^4.8.3",
    "tailwindcss": "^4.1.18",
    "vite": "npm:rolldown-vite@^7.3.0",
    "vue": "^3.5.26",
    "vue-infinity": "^0.7.3",
    "vue-maplibre-gl": "^5.5.9",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.50.0",
    "@types/wicg-file-system-access": "^2023.10.7",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "eslint": "^9.29.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-vue": "~10.2.0",
    "httpxy": "^0.1.7",
    "jiti": "^2.6.1",
    "jsdom": "^26.1.0",
    "knip": "^5.83.1",
    "monocart-reporter": "^2.9.0",
    "prettier": "3.7.4",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "typescript": "~5.8.3",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vue-tsc": "^2.2.12"
  },
  "packageManager": "pnpm@10.18.3"
}
